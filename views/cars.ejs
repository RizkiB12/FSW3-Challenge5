<div class="d-flex justify-content-between">
    <h3>
       <b>List Cars</b>
    </h3>
    <button type="button" class="btn btn-primary">
        <a href="addNewCar.ejs"></a>+ Add New Car
    </button>
</div>

<div class="mt-3">
    <button class="btn btn-outline-primary p"><a href=""></a>All</button>
    <button class="btn btn-outline-primary"><a href=""></a>Small</button>
    <button class="btn btn-outline-primary"><a href=""></a>Medium</button>
    <button class="btn btn-outline-primary"><a href=""></a>Large</button>
</div>

<div class="row m-0 p-0 mt-3">
    <% arrayCars.forEach(car => { %>
    <div class="col-md-4 p-2">
        <div class="card">
            <img src="<%= car.img %>" alt="" class="card-img-top">
            <div class="card-body">
                <p><%= car.nama %></p>
                <p class="fw-bold">Rp. <%= car.rent %> / day</p>
                <p class="text-secondary"><i class="fa fa-clock-o"></i> Updated at 4 Apr 2022, 09.00</p>
                <div class="d-inline-flex gap-2">
                    <button class="btn btn-outline-danger"><i class="fa fa-trash"></i> Delete</button>
                    <button class="btn btn-success text-white"><i class="fa fa-edit"></i> Edit</button>
                </div>
            </div>
        </div>
    </div>
    <% }) %>
</div>